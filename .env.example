# Development environment variables
# Copy this to .env.local and update with your values

# Cloudflare Account ID (found in Cloudflare dashboard)
CLOUDFLARE_ACCOUNT_ID=your-account-id-here

# Cloudflare API Token (create at https://dash.cloudflare.com/profile/api-tokens)
CLOUDFLARE_API_TOKEN=your-api-token-here

# KV Namespace ID (create with: wrangler kv:namespace create "CACHE")
KV_NAMESPACE_ID=your-kv-namespace-id-here
KV_NAMESPACE_PREVIEW_ID=your-kv-namespace-preview-id-here
